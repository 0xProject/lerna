{
  "packages": [
    "commands/*",
    "core/*",
    "utils/*"
  ],
  "command": {
    "create": {
      "homepage": "https://github.com/0xproject/lerna",
      "license": "MIT"
    },
    "version": {
      "conventionalCommits": true,
      "exact": true,
      "message": "chore(release): %s"
    }
  },
  "ignoreChanges": [
    "**/__fixtures__/**",
    "**/__tests__/**",
    "**/*.md"
  ],
  "version": "3.16.7"
}
